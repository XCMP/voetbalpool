<div class="table">
  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Logo</th>
      </tr>
    </thead>
    <tbody>
      {{#each clubs}}
        <tr class="club" data-id="{{_id}}">
          <td>{{name}}</td>
          <td><img class="small" src="/images/logos/{{logoFilename}}"/></td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>
<div class="buttons">
  <button type="button" class="js_button_add">Toevoegen</button>
  <button type="button" class="js_button_update" disabled>Wijzigen</button>
  <button type="button" class="js_button_delete" disabled>Verwijderen</button>
</div>