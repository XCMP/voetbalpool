<select name="{{name}}" class="club_select">
  <option value="" selected disabled>kies een club</option>
  {{#each clubs}}
    <option value="{{_id}}" {{#if selected}}selected{{/if}}>{{name}}</option>
  {{/each}}
</select>
<img class="logo small {{#unless selected}}hide {{/unless}}padding-left-s {{name}}" src="/images/logos/{{selected.logoFilename}}"/>

